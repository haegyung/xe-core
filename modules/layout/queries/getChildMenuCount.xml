<query id="getChildMenuCount" action="select">
    <tables>
        <table name="layout_menu" />
    </tables>
    <columns>
        <column name="count(*)" alias="count" />
    </columns>
    <conditions>
        <condition operation="equal" column="parent_srl" var="menu_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
