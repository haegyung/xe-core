<?xml version="1.0" encoding="utf-8"?>
<ruleset version="1.5.0">
    <customrules>
		<rule name="ip" type="regex" test="/((0|1[0-9]{0,2}|2[0-4][0-9]|25[0-5])|\*)(\.((0|1[0-9]{0,2}|2[0-4][0-9]|25[0-5])|\*)){3}/"  />
    </customrules>
    <fields>
		<field name="ipaddress" required="true" rule="ip" />
    </fields>
</ruleset>
