<filter name="logout" module="board" act="procLogout">
  <form>
    <node target="mid" required="true" />
    <node target="act" required="true" />
  </form>
  <parameter>
    <param name="mid" target="mid" />
    <param name="act" target="act" />
  </parameter>
  <response>
    <tag name="error" />
    <tag name="message" />
  </response>
</filter>
