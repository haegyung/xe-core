<query id="getFiles" action="select">
  <tables>
    <table name="files" />
  </tables>
  <conditions>
    <condition operation="equal" column="document_srl" var="document_srl" filter="number" notnull="notnull" />
  </conditions>
  <navigation>
    <index var="sort_index" order="asc" />
  </navigation>
</query>
