<query id="insertDriverConfig" action="insert">
    <tables>
        <table name="driver_config" />
    </tables>
    <columns>
        <column name="module" var="module" notnull="notnull" />
        <column name="driver" var="driver" notnull="notnull" />
        <column name="module_srl" var="moduleSrl" />
        <column name="config" var="config" />
        <column name="regdate" default="curdate()" />
    </columns>
</query>
